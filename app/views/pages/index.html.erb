<%= render 'partials/nav' %>
<p id="notice"><%= notice %></p>

<div class = "welcomeFairtrade">
   <h1>Welcome to Fairtrade page</h1>
</div>

   </div>
</div>

<h2>New arrival</h2>
<br>
<div class="row">
<% @posts.reverse[0..3].each do |post| %>
	<div class="col-xs-6 col-md-3">
		<div class="thumbnail">
          <img src ="<%= @post.images %>" alt="Image">
        <%= @post.inspect %>
			<div class="caption">
				<h4><%= post.item_name %><h4>
        <br>
        <%= link_to "See more", post_path(post.id) %>
        <br><br>
        <%= render 'partials/stripe', post: post %>
        <!-- <div class="btn-group" data-toggle="buttons-checkbox">
          <button type="button" class="btn btn-primary-see-more"><%= link_to "See more", post_path(post.id) %></button>
          <button type="button" class="btn btn-primary-purchase">Purchase</button>
        </div> -->
			</div>
		</div>
	</div>
    <% end %>
</div>
